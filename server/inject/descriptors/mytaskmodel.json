      "selector": ".panel-header-inside h1",
      "selectContains": "My Tasks"
    },
    "myTasks": {
      "type": "object",
      "selector": "#myTask-body",
      "properties": {
        "taskDescription": {
          "type": "string",
          "selector": ".panel-header",
          "control": "property.innerText"
        }
      }
    },
    "myDelegates": {
      "type": "object",
      "selector": "#myDelegate-body",
      "properties": {
        "delegateDescription": {
          "type": "string",
          "selector": "h1",
          "control": "property.innerHTML"
        }
      }
    }
  },
  "connect": []
}
