{
  "type": "object",
  "comment": "",
  "properties": {
    "ismytask": {
      "type": "identifier",
      "control": "exists",
      "selector": ".panel-header-inside h1",
      "selectContains": "My Tasks"
    },
    "mytask": {
      "type": "array",
      "selectorAll": ".result-data",
      "items": {
        "type": "object",
        "properties": {
          "processname": {
            "type": "string",
            "control": "property.innerText",
            "selector": "strong"
          },
          "activities": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "activityname": {
                  "type": "string",
                  "selector": "div",
                  "control": "property.innerText"
                }
              }
            },
            "selectorAll": {
              "fn": "getActivities",
              "selectorAll": ""
            }
          }
        }
      }
    },
    "clickCar": {
      "type": "action",
      "selector": "#app-id-1.search-result-item.dark-gray.selected-task",
      "control": "action.button"
    }
  },
  "connect": []
}
